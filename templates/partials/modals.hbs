<div class="modal fade" id="onboardModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalSignTitle"
    aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-sm" role="document">
        <div class="modal-content">
            <div class="modal-body p-0">
                <div class="card card-plain">
                    <div class="card-header pb-0 text-left">
                        <h5 class="">Add New Subject</h5>
                        <p class="mb-0">Enter UID of Subject</p>
                    </div>
                    <div class="card-body pb-3">
                        <form role="form text-left">
                            <div class="input-group input-group-outline my-3">
                                <label class="form-label">Name</label>
                                <input id="uid" type="text" class="form-control">
                            </div>
                            <div class="text-center">
                                <button type="button" id="onboardbutton"
                                    class="btn bg-gradient-primary btn-lg btn-rounded w-100 mt-4 mb-0">Create</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="onboardModalSuccess" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title font-weight-normal" id="exampleModalLabel">Success</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                User has been successfully created !!
            </div>
            <div class="modal-footer">
                <button type="button" class="btn bg-gradient-primary" data-bs-dismiss="modal"
                    onclick="window.location.reload()">Close and Reload</button>
            </div>
        </div>
    </div>
</div>

